{
   "extends": "eslint-config-airbnb",
   "rules": {
      "indent": ["error", 3],
      "react/jsx-indent": ["error", 3],
      "comma-dangle": ["error", "never"],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "no-use-before-define": ["error", { "functions": false, "classes": false, "variables": false }]
   }
}
